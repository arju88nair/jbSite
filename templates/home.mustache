<script src="assets/js/home_page_Api.js?v=2.1"></script>
<script>
    var jump=function(e)
    {
        if (e){
            e.preventDefault();
            var target = $(this).attr("href");
        }else{
            var target = location.hash;
        }
//        if(target == "#membershipPlans")
//        {
//            $('html, body').animate({
//                scrollTop: $(target).offset().top - 150
//            },800);
//            location.hash = target;
//
//
//            $(target+"Menu").addClass('current-menu-parent');
//
//        }

        $('html,body').animate(
                {
                    scrollTop: $(target).offset().top
                },2000,function()
                {
                    location.hash = target;
                });

    }
    $(document).ready(function()
    {
        logClick("On load","Home","Home");
        $('a[href^=#membershipPlans]').bind("click", jump);

        if (location.hash){
            setTimeout(function(){
                $('html, body').scrollTop(0).show();
                jump();
            }, 0);
        }else{
            $('html, body').show();
        }
        $('#home').addClass('current-menu-parent');

    })


</script>

